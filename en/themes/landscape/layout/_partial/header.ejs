<header id="header">
  <div id="banner"></div>
  <div id="header-outer" class="outer">
    <div id="header-title" class="inner">
      <h1 id="logo-wrap">
        <a href="<%- url_for() %>" id="logo"><%= config.title %></a>
      </h1>
      <% if (theme.subtitle){ %>
        <h2 id="subtitle-wrap">
          <a href="<%- url_for() %>" id="subtitle"><%= theme.subtitle %></a>
        </h2>
      <% } %>

	    </div>
	
  	
    <div id="header-inner" class="inner">
      <nav id="main-nav">
        <a id="main-nav-toggle" class="nav-icon" ></a>
        <% for (var i in theme.menu){ %>
          <a class="main-nav-link" href="<%- url_for(theme.menu[i]) %>"><%= i %></a>
		  
        <% } %>
      </nav>
	  <nav id="sub-nav">
	  <a class="nav-icon" href="" title="本站Logo" target="_blank"><img src="https://i.loli.net/2017/08/20/5998f25bbda69.jpg" width=30px height=20px></a>
	  
	  <a class="nav-icon" href="https://PhDmeiwp.github.io/" title="中文版" target="_blank"><img src="http://i2.muimg.com/567571/03dab2a799c7e397.png" width=30px height=20px></a>
	  <a class="nav-icon" href="https://meiweiping.github.io/" title="English version" target="_blank"><img src="http://i2.muimg.com/567571/194eed5444045198.png" width=30px height=20px></a>
	  
</nav>
     </div>
  </div>
  
 
